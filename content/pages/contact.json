{
  "path": "/contact",
  "label": "Contact",
  "layout": "1col",
  "sections": [
    {
      "title": "Contactez nous",
      "color": "#000",
      "align": "center",
      "margin": "2",
      "tag": "h1",
      "_template": "TitleBlock"
    },
    {
      "flexAlign": "flex-start",
      "flexReverse": false,
      "hpadding": 0,
      "vpadding": 0,
      "hmargin": 0,
      "vmargin": 0,
      "cols": [
        {
          "lat": 40,
          "lng": 40,
          "zoom": 13,
          "height": 300,
          "width": 300,
          "flex": 2,
          "hmargin": 0,
          "vmargin": 0,
          "hpadding": 0,
          "vpadding": 0,
          "_template": "MapBlock",
          "flexMap": true
        },
        {
          "errorMessage": "Something fail :(",
          "submitLabel": "Submit",
          "successMessage": "Success !!!",
          "fields": [
            {
              "label": "Nom",
              "type": "text",
              "required": true,
              "name": "name",
              "errorMessage": "error",
              "fieldErrorMessage": "Entrez votre nom svp"
            },
            {
              "type": "email",
              "name": "email",
              "label": "Email",
              "errorMessage": "error",
              "required": true,
              "fieldErrorMessage": "Entrez une adresse email valide svp"
            },
            {
              "type": "textarea",
              "name": "message",
              "label": "Votre message",
              "errorMessage": "error",
              "required": true,
              "fieldErrorMessage": "Entrez un message svp"
            }
          ],
          "flex": 3,
          "hmargin": 0,
          "vmargin": 0,
          "hpadding": 3,
          "vpadding": 3,
          "_template": "FormBlock"
        }
      ],
      "_template": "RowBlock"
    }
  ]
}